{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.villager_v2_base": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.base",
            "Texture.base2",
            "Texture.base3",
            "Texture.base4",
            "Texture.base5",
            "Texture.base6"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.default" }
      ],
      "textures": [
        "Array.skins[query.skin_id]"
      ],
      "is_hurt_color": {
        "r": "1.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.35"
      },
      "on_fire_color": {
        "r": "0.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.0"
      }
    },
    "controller.render.villager_v2_masked": {
      "arrays": {
        "textures": {
          "Array.biomes": [ 
            "Texture.plains",
            "Texture.desert",
            "Texture.jungle",
            "Texture.savanna",
            "Texture.snow",
            "Texture.swamp",
            "Texture.taiga"
          ],
          "Array.professions": [
            "Texture.unskilled",
            "Texture.farmer",
            "Texture.fisherman",
            "Texture.shepherd",
            "Texture.fletcher",
            "Texture.librarian",
            "Texture.cartographer",
            "Texture.cleric",
            "Texture.armorer",
            "Texture.weapon_smith",
            "Texture.tool_smith",
            "Texture.butcher",
            "Texture.leatherworker",
            "Texture.stonemason",
            "Texture.nitwit"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        { "*": "query.is_baby ? Material.default : Material.masked" }
      ],
      "textures": [
        "Array.biomes[query.mark_variant]",
        "Array.professions[variable.profession_index]"
      ],
      "is_hurt_color": {
        "r": "1.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.35"
      },
      "on_fire_color": {
        "r": "0.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.0"
      }
    },
    "controller.render.villager_v2_biome": {
      "arrays": {
        "textures": {
          "Array.biomes": [ 
            "Texture.plains",
            "Texture.desert",
            "Texture.jungle",
            "Texture.savanna",
            "Texture.snow",
            "Texture.swamp",
            "Texture.taiga"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.default" }
      ],
      "part_visibility": [
        { "*": true },
        { "head": "variable.hat_visible" },
        { "helmet": "variable.hat_visible" },
        { "brim": "variable.hat_visible" },
        { "nose": "variable.hat_visible" }
      ],
      "textures": [
        "Array.biomes[query.mark_variant]"
      ],
      "is_hurt_color": {
        "r": "1.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.35"
      },
      "on_fire_color": {
        "r": "0.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.0"
      }
    },
    "controller.render.villager_v2_profession": {
      "arrays": {
        "textures": {
          "Array.professions": [
            "Texture.unskilled",
            "Texture.farmer",
            "Texture.fisherman",
            "Texture.shepherd",
            "Texture.fletcher",
            "Texture.librarian",
            "Texture.cartographer",
            "Texture.cleric",
            "Texture.armorer",
            "Texture.weapon_smith",
            "Texture.tool_smith",
            "Texture.butcher",
            "Texture.leatherworker",
            "Texture.stonemason",
            "Texture.nitwit"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.default" }
      ],
      "part_visibility": [
        { "*": "(!query.is_baby && variable.profession_index != 0)" }
      ],
      "textures": [
        "Array.professions[variable.profession_index]"
      ],
      "is_hurt_color": {
        "r": "1.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.35"
      },
      "on_fire_color": {
        "r": "0.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.0"
      }
    },
    "controller.render.villager_v2_level": {
      "arrays": {
        "textures": {
          "Array.levels": [
            "Texture.level_stone",
            "Texture.level_iron",
            "Texture.level_gold",
            "Texture.level_emerald",
            "Texture.level_diamond"
          ]
        }
      },
      "rebuild_animation_matrices": true,
      "geometry": "Geometry.default",
      "part_visibility": [ { "*": "!query.is_baby && variable.profession_index != 0 && variable.profession_index != 14" } ],
      "materials": [
        { "*": "Material.default" }
      ],
      "textures": [
        "Array.levels[variable.level_index]"
      ],
      "is_hurt_color": {
        "r": "1.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.35"
      },
      "on_fire_color": {
        "r": "0.0",
        "g": "0.0",
        "b": "0.0",
        "a": "0.0"
      }
    }
  }
}