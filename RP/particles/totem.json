        "facing_camera_mode": "rotate_xyz",
        "uv": {
          "texture_width": 128,
          "texture_height": 128,
          "flipbook": {
            "base_UV": [ 64, 88 ],
            "size_UV": [ 8, 8 ],
            "step_UV": [ -8, 0 ],
            "frames_per_second": 8,
            "max_frame": 8,
            "stretch_to_lifetime": true,
            "loop": false
          }
        }
      },
    "minecraft:particle_appearance_tinting": {
        "color": [ "((variable.particle_random_4 * variable.particle_random_4) * 0.3 + 0.7 - variable.particle_random_2 * variable.particle_random_2 * 0.3) * (variable.particle_random_3 > 0.6 ? (variable.particle_random_3 * 1.3 > 1 ? 1 : variable.particle_random_3 * 1.3) : variable.particle_random_3 * 0.2)", "((variable.particle_random_4 * variable.particle_random_4) * 0.2 + 0.7 - variable.particle_random_2 * 0.25)" , 0.0, "variable.particle_age / variable.particle_lifetime > 0.5 ? 1.5 - variable.particle_age / variable.particle_lifetime : 1.0" ]
      }
    }
